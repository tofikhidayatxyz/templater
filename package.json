{
  "main": "bin/compiler.js",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"cross-env NODE_ENV=development node bin/compiler.js\" \"cross-env NODE_ENV=development node server.js\"",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "compile": "cross-env NODE_ENV=production node bin/compiler.js",
    "publish": "concurrently \"npm run prod\" \"npm run compile\"",
    "publish-minify": "concurrently \"npm run prod\" \"cross-env MINIFY=true node bin/compiler.js\"",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "tofikhidayat",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.12.4",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "edge.js": "^1.1.4",
    "fs": "0.0.2",
    "fs-extra": "^9.0.1",
    "html-formatter": "^0.1.9",
    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.1",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.5",
    "live-server": "^1.2.1",
    "lodash": "^4.17.21",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "pretty": "^2.0.0",
    "recursive-readdir": "^2.2.2",
    "sass-loader": "^10.0.2",
    "vue-template-compiler": "^2.6.12",
    "turbolinks": "^5.2.0"
  },
  "browserslist": [
    "> 1%",
    "iOS >= 8",
    "Safari >= 8",
    "ie >= 9",
    "ie 7",
    "ie 6",
    "last 2 versions"
  ]
}
